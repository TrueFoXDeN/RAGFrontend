{
  "name": "ragfrontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format": "prettier --write src/**/*.{ts,html} && sass-lint src/**/*.sass",
    "clean:apigen": "rimraf ./src/app/api-client",
    "apigen": "npm run clean:apigen && openapi-generator-cli generate -i openapi.json -g typescript-angular -o ./src/app/api-client"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "marked": "^12.0.2",
    "ngx-markdown": "^18.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.10",
    "prismjs": "^1.28.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^10.0.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.5",
    "@angular/cli": "^18.2.5",
    "@angular/compiler-cli": "^18.2.0",
    "@openapitools/openapi-generator-cli": "^2.13.9",
    "@types/jasmine": "~5.1.0",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sass-lint": "^1.13.1",
    "tailwindcss": "^3.4.12",
    "typescript": "~5.5.2"
  }
}
