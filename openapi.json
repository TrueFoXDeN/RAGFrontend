  "components": {
    "schemas": {
      "ChatRequest": {
        "properties": {
          "chat_id": {
            "type": "string",
            "title": "Chat Id"
          },
          "summary": {
            "type": "string",
            "title": "Summary"
          },
          "messages": {
            "items": {
              "$ref": "#/components/schemas/Message"
            },
            "type": "array",
            "title": "Messages"
          }
        },
        "type": "object",
        "required": [
          "chat_id",
          "summary",
          "messages"
        ],
        "title": "ChatRequest"
      },
      "Context": {
        "properties": {
          "file": {
            "type": "string",
            "title": "File"
          },
          "page": {
            "type": "integer",
            "title": "Page"
          },
          "text": {
            "type": "string",
            "title": "Text"
          }
        },
        "type": "object",
        "required": [
          "file",
          "page",
          "text"
        ],
        "title": "Context"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "Message": {
        "properties": {
          "context": {
            "items": {
              "$ref": "#/components/schemas/Context"
            },
            "type": "array",
            "title": "Context"
          },
          "text": {
            "type": "string",
            "title": "Text"
          },
          "type": {
            "$ref": "#/components/schemas/MessageType"
          }
        },
        "type": "object",
        "required": [
          "context",
          "text",
          "type"
        ],
        "title": "Message"
      },
      "MessageType": {
        "type": "string",
        "enum": [
          "prompt",
          "response"
        ],
        "title": "MessageType"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      }
    }
  }
}
